{
    "template": "<div class=\"container-fluid\">\n  <div ng-init=\"\">\n    <link rel=\"stylesheet\" type=\"text/css\" href=\"{{config.server_path}}/css/weather12345.css\" />\n  </div>\n  \n  <div class=\"row\">\n    <div oc-lazy-load=\"['../static/weather12345/js/unitctrl.js']\"  >\n      <div id=\"checkUnits\" class=\"{{config.units}}\">\n        <div ng-controller='UnitCtrl'>\n          <div class=\"col-xs-12\">\n            <div class=\"text-left weather-location\">\n              {{itemValue('Weather_Location_Name')}}\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n  <div class=\"row horizontalrule\">\n    <div class=\"col-xs-12\">\n      <div class=\"weather-current-temp\">\n        {{itemValue('Weather_Temp_C')}} °C\n      </div>    \n    </div>\n  </div>\n  <div class=\"row\">\n    <div class=\"col-xs-12\">\n      <div class=\"weather-current-feel\">\n        Feels {{itemValue('Weather_Feel_C')}} °C\n      </div>     \n    </div>\n  </div>\n  <div class=\"row\">\n    <div class=\"col-xs-12\">\n      <img class=\"weather-icon pulse\" src=\"{{config.server_path}}/images/{{config.IconSet}}/{{itemValue('Weather_IconToday') | lowercase }}.png\"/>   \n    </div>\n  </div>\n  <div class=\"row\">\n    <div class=\"text-left weather-current-details\">\n      Precip: {{itemValue('Weather_PrecipToday') }} mm\n    </div>\n  </div>\n  <div class=\"row\">\n    <div class=\"text-left weather-current-details\">\n      Chill: {{itemValue('Weather_Wind_ChillC') }} °C\n    </div>\n  </div>\n  <div class=\"row\">\n    <div class=\"text-left weather-current-details\">\n      Humidity: {{itemValue('Weather_Humidity')}} %\n    </div>\n  </div>\n  <div class=\"row\">\n    <div class=\"temp-current temp-current-column text-left weather-current-details\">\n      <span class=\"temp-max\">{{itemValue('Weather_Today_TempHighC')}}</span><span> | </span><span class=\"temp-min\">{{itemValue('Weather_Today_TempLowC')}} </span><span>°C</span>\n    <